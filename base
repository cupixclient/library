	function Cupix_Lib:CreateButton(Name,Callback)
		Callback = Callback or function() end
		local MainBtn = Instance.new("ImageButton")
		local ButtonContent = Instance.new("TextLabel")
		
		MainBtn.Name = math.random()
		MainBtn.Parent = PageContents
		MainBtn.BackgroundTransparency = 1
		MainBtn.ImageTransparency = 0
		MainBtn.Image = "rbxassetid://8543726385"
		MainBtn.ImageColor3 = Color3.fromRGB(35, 35, 35)
		MainBtn.BorderSizePixel = 0
		MainBtn.Position = UDim2.new(0.231626838, 0, 1.09619045, 0)
		MainBtn.Size = UDim2.new(0, 296, 0, 32)

		ButtonContent.Name = math.random()
		ButtonContent.Parent = MainBtn
		ButtonContent.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		ButtonContent.BackgroundTransparency = 1
		ButtonContent.Size = UDim2.new(0, 296, 0, 32)
		ButtonContent.Font = Enum.Font.GothamSemibold
		ButtonContent.Text = Name
		ButtonContent.TextColor3 = Color3.fromRGB(255, 255, 255)
		ButtonContent.TextSize = 13.000
		
		local function YNZEUT_fake_script() -- MainBtn.HoverManager 
			local script = Instance.new('LocalScript', MainBtn)

			local Parent = MainBtn
			Parent.MouseEnter:Connect(function()
				local Speed = 0.3
				local TweenFunc = TweenInfo.new(Speed, Enum.EasingStyle.Quad , Enum.EasingDirection.InOut, 0)
				local TweenServ = game:GetService("TweenService")
				local Hovered = 0.5
				TweenServ:Create(Parent,TweenFunc, {ImageTransparency = Hovered}):Play()
			end)

			Parent.MouseLeave:Connect(function()
				local Speed = 0.3
				local TweenFunc = TweenInfo.new(Speed, Enum.EasingStyle.Quad , Enum.EasingDirection.InOut, 0)
				local TweenServ = game:GetService("TweenService")
				local UnHovered = 0
				TweenServ:Create(Parent,TweenFunc, {ImageTransparency = UnHovered}):Play()
			end)
			
			Parent.MouseButton1Click:Connect(function()
				pcall(Callback)
			end)
		end
		coroutine.wrap(YNZEUT_fake_script)()
	end
